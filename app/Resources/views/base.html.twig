            <script src="{{ asset('assets/plugins/magnific-popup/magnific-popup.min.js') }}"></script>
            <script src="{{ asset('assets/plugins/owl-carousel/owl.carousel.min.js') }}"></script>
            <script src="http://maps.google.com/maps/api/js?key=AIzaSyDCOgtrXxCRLynF9eWFMdHmcxgEBg4y8Gg"></script>
            <script src="../assets/plugins/gmaps.min.js{{ asset('assets/plugins/gmaps.min.js') }}"></script>

