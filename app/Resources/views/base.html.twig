    <body data-preloader="2">
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('assets/plugins/jquery.min.js') }}"></script>
            <script src="{{ asset('assets/plugins/bootstrap/popper.min.js') }}"></script>
